{
  "private": true,
  "name": "tinkering-with-durable-objects",
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "description": "Tinkering with some durable objects",
  "module": "./dist/index.mjs",
  "scripts": {
    "dev": "yarn compile --watch",
    "compile": "tsc --build",
    "clean": "rimraf **/dist **/tsconfig.tsbuildinfo **/yarn-error.log **/yarn.lock",
    "test": "lerna run test --stream",
    "barrel": "lerna run barrel --stream",
    "site": "cd packages/site && yarn run",
    "server": "cd packages/server && yarn run",
    "site-worker": "cd packages/site-worker && yarn run"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.10",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "lerna": "^4.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "author": "mike.cann@gmail.com",
  "license": "MIT"
}
